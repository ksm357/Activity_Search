<div style="position: relative">
    <div class="w3-content w3-section">
          <img class="mySlides w3-animate-top" src="<%= image_url("background_yoga1.jpg") %>" style="height: 750px; width: 100%;">
          <img class="mySlides w3-animate-top" src="<%= image_url("background_pingpong.jpg") %>" style="height: 750px; width: 100%;">
          <img class="mySlides w3-animate-top" src="<%= image_url("background_bowling1.jpg") %>" style="height: 750px; width: 100%;">
          <img class="mySlides w3-animate-top" src="<%= image_url("background_surfing2.jpg") %>" style="height: 750px; width: 100%;">
          <img class="mySlides w3-animate-top" src="<%= image_url("background_basketball.jpg") %>" style="height: 750px; width: 100%;">
          <img class="mySlides w3-animate-top" src="<%= image_url("background_baseball.jpg") %>" style="height: 750px; width: 100%;">
          <img class="mySlides w3-animate-top" src="<%= image_url("background_swimming.jpg") %>" style="height: 750px; width: 100%;">
    </div>
    <div style="position: absolute; bottom: 30px; left: 25%; width: 50%; text-align: center;">
            
        <div class="over_slideshow">
            <h1> Do not hesitate. Act now! </h1>
            <br><br>
            <p>로그인을 하면 더 많은 서비스를 이용할 수 있습니다.</p> 
        </div>
    </div>
</div>


<script>
var myIndex = 0;
carousel();

function carousel() {
    var i;
    var x = document.getElementsByClassName("mySlides");
    for (i = 0; i < x.length; i++) {
       x[i].style.display = "none";  
    }
    myIndex++;
    if (myIndex > x.length) {myIndex = 1}    
    x[myIndex-1].style.display = "block";  
    setTimeout(carousel, 4000); 
    
}
</script>
