<%= form_for @question do |f| %>
  
  <%= f.hidden_field :user_id, value: current_user.id  %> 
  <p>
    제목 : <%= f.text_field :questionTitle, label: '제목' %>
  </p>
 
  <p>
    내용 : <%= f.text_area :questionContent, label: 'Memo', class: "tinymce", rows: 15, cols: 120 %>
  </p>
    <%= tinymce %> 
  <p>
    <%= f.submit %>
  </p>
<% end %>